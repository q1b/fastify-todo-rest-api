{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "github": { "silent": true, "autoJobCancelation": true },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless.ts"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
